<?xml version="1.0" encoding="utf-8"?>
<mujoco> 
    <compiler angle="radian" coordinate="local" meshdir="fetch" texturedir="texture"></compiler>
    <size nconmax="2000" njmax="1000"/>
    <option timestep="0.001">
		<flag warmstart="enable"></flag>
        
	</option>
    <include file="shared.xml"></include>
    <worldbody> 
        <geom name="floorgeom0" pos="1.2 -4.00 0" size="1.0 8.0 1" type="plane" condim="3" material="floor_mat"></geom>
        <body name="floor0" pos="1.655 0.25 0">
			<site name="target0" pos="0 0.079 0.7" size="0.0475 0.001" rgba="1 0 0 1" type="cylinder"></site>
		</body>
        <body pos="0.75 0 0.78" name="pedestal0">
			<geom name="pedestalgeom0" size="0.4 0.4 0.02" type="box" mass="2000" material="pedestal_mat"></geom>
			<site pos="0.30 0.30 0" size="0.075 0.075 0.002" type="box" name="robotmountsite0" rgba="0.55 0.54 0.53 1" />
		</body>
        
        <include file="robot_xarm.xml"></include>
        
