            <geom name="tablegeom0" size="0.4 0.425 0.04375" type="box" mass="2000" material="table_mat"></geom>
			<geom pos = "0 -0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary1" rgba="0.90 0.75 0.51 1" material="table_mat"/> 
			<geom pos = "0 0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary2" rgba="0.90 0.75 0.51 1" material="table_mat"/>
