        </body> -->
        
        <body pos="1.655 0.3 0.65625" name="table0">
            <geom name="tablegeom0" size="0.4 0.425 0.04375" type="box" material="table_mat" density="2000" friction="1 0.005 0.0001"></geom>
            <geom pos = "0 -0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary1" rgba="0.90 0.75 0.51 1" material="table_mat" density="2000"></geom>
            <geom pos = "0 0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary2" rgba="0.90 0.75 0.51 1" material="table_mat" density="2000"></geom>
            <geom pos = "-0.40 0 0.04375" size="0.02 0.425 0.02" type="box" name="boundary3" rgba="0.90 0.75 0.51 1" material="table_mat" density="2000"></geom>
            <site pos = "0 -0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary1" rgba="0.90 0.75 0.51 1" material="table_mat"/> 
            <site pos = "0 0.38 0.04375" size="0.4 0.05 0.04" type="box" name="boundary2" rgba="0.90 0.75 0.51 1" material="table_mat"/>
            <site pos = "-0.40 0 0.04375" size="0.02 0.425 0.02" type="box" name="boundary3" rgba="0.90 0.75 0.51 1" material="table_mat"/>
            <!-- <site pos="-0.205 0.0 0.04375" size="0.1 0.15 0.002" type="box" rgba="0 0 0 1" material="table_mat"/> -->
        </body>


        <body name="box_hole" pos="1.605 0.25 0.73">
            <joint name="box_hole:joint" type="free" limited="false"></joint>
            <geom name="box_left" pos="0 0 0" size="0.075 0.002 0.04" type= "box" rgba="0.8 0 0 1" mass ="10000" friction="0.95 0.3 0.1"/>
            <geom name ="box_right" pos="0 0.149 0" size="0.075 0.002 0.04" type="box" rgba="0.8 0 0 1" mass ="10000" friction="0.95 0.3 0.1" />
            <geom name="box_forward" pos="0.074 0.074 0" size="0.002 0.075 0.04" type="box" rgba="0.8 0 0 1" mass ="10000" friction="0.95 0.3 0.1" />
            <geom name="box_backward" pos="-0.074 0.074 0" size="0.002 0.075 0.04" type="box" rgba="0.8 0 0 1" mass ="10000" friction="0.95 0.3 0.1" />
            <geom name="box_bottom" pos="-0 0.074 -0.038" size="0.075 0.075 0.002" type="box" rgba="0.8 0 0 1" mass ="10000" friction="0.95 0.3 0.1"/>
        </body>

        <body name="object0" pos="1.4 0.25 0.8">
            <joint name="object0:joint" type="free" limited="false"></joint>
            <geom name="object0_target" type="cylinder" pos="0 0 -0.03" size="0.03 0.03" rgba="0.6 0.8 0.5 1" density="10000" friction="0.95 0.3 0.1"/>
            <site name="object0" pos="0 0 0" size="0.035 0.035 0.035" rgba="0 0 0 0" type="sphere"></site>
            <body name="B0" pos="0 0 0" euler="0 -1.57 0 ">
                <joint name="B0:joint" type="slide" limited="true" axis="0 0 1" range="0.0001 0.0002" frictionloss="1"></joint>
                <composite type="rope" count="10" spacing="0.015" offset="0 0 0">
                    <joint kind="main" damping="0.005" solreffix="0.02 1" frictionloss="1" solimpfix="0.99 0.99 0.99" solimpfriction="0.98 0.98 0.95"/>
                    <geom type="capsule" size="0.0023 0.005" rgba="0 0 0 1" mass="0.01"/>
                </composite>
            </body>

        </body>
       
        
        <light directional="true" ambient="0.2 0.2 0.2" diffuse="0 0 0" specular="0.01 0.01 0.01" castshadow="false" pos="4 0 4" dir="-1 0 -0.1" name="light0"></light>
        <light directional="true" ambient="0.1 0.1 0.1" diffuse="0 0 0" specular="0.01 0.01 0.01" castshadow="false" pos="0 -4 4" dir="0 1 -0.1" name="light1"></light>
        <light directional="true" ambient="0 0 0" diffuse="0 0 0" specular="0.01 0.01 0.01" castshadow="false" pos="40 4 4" name="light2"></light>
        <light pos="1.3 0 2" castshadow="false" name="light3"></light>
        
        <camera fovy="70" name="third_person" pos="2.10 0.3 1.5" euler="0 0.95 1.57"/> <!-- could be 1.5 height -->
    </worldbody>


    <equality>
        <connect body2="left_finger" body1="left_inner_knuckle" anchor="0.0 0.035 0.042" solimp="0.9 0.95 0.001 0.5 2" solref="0.0002 1.0" ></connect>
        <connect body2="right_finger" body1="right_inner_knuckle" anchor="0.0 -0.035 0.042" solimp="0.9 0.95 0.001 0.5 2" solref="0.0002 1.0" ></connect>
        <weld body1="robot0:mocap2" body2="B9" solimp="0.99 0.99 0.99" solref="0.02 1"></weld>
        
        <joint joint1="left_inner_knuckle_joint" joint2="right_inner_knuckle_joint"></joint>
    </equality>
    <actuator>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="left_inner_knuckle_joint" gear="200.0"/>
        <motor ctrllimited="true" ctrlrange="-1.0 1.0" joint="right_inner_knuckle_joint" gear="200.0"/>
        <!-- <position ctrllimited="true" ctrlrange="0 0.85" gear="1" kp="10" joint="left_inner_knuckle_joint"/> -->
    </actuator>
</mujoco>
